{
        "version": 2,
        "builds": [
          {
            "src": "employment_contracts/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
          },
          {
            "src": "build_files.sh",
            "use": "@vercel/static-build"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "employment_contracts/wsgi.py",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
          }
        ]

      }
      